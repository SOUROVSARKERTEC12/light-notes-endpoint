generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


model User {
  id          String   @id @default(uuid())
  email       String   @unique
  password    String
  firstName   String
  lastName    String
  phoneNumber String?      
  dateOfBirth DateTime?    
  bio         String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}